<div class="container" style="width:{page:width}">
    <div class="headb">{lang:mod} - {lang:head_version}</div>
    <div class="leftb">{lang:body_version}</div>
</div>
<br />
<table class="forum" cellpadding="0" cellspacing="1" style="width:{page:width}">
  <tr>
    <td class="headb" style="width:40%">{lang:status}</td>
    <td class="headb">{lang:version}</td>
    <td class="headb">{lang:date}</td>
  </tr>
  <tr>
    <td class="leftc">{lang:installed}</td>
    <td class="leftb">{version:version}</td>
    <td class="leftb">{version:version_date}</td>
  </tr>
  <tr>
    <td class="leftc">{lang:available}</td>
    <td class="leftb">{version:available}</td>
    <td class="leftb">{version:available_date}</td>
  </tr>
</table>
<br />
<div class="container" style="width:{page:width}">
    <div class="centerb">{version:msg}</div>
</div>
