<table class="forum" cellpadding="0" cellspacing="{page:cellspacing}" style="width:{page:width}">
  <tr>
    <td class="headb">{lang:mod_name} - {lang:head_version}</td>
  </tr>
  <tr>
    <td class="leftb">{lang:body_version}</td>
  </tr>
</table>
<br />
<table class="forum" cellpadding="0" cellspacing="{page:cellspacing}" style="width:{page:width}">
  <tr>
    <td class="headb" style="width:40%">{lang:status}</td>
    <td class="headb">{lang:version}</td>
    <td class="headb">{lang:date}</td>
  </tr>
  <tr>
    <td class="leftc">{lang:installed}</td>
    <td class="leftc">{version:version}</td>
    <td class="leftc">{version:version_date}</td>
  </tr>
  <tr>
    <td class="leftc">{lang:available}</td>
    <td class="leftc">{version:available}</td>
    <td class="leftc">{version:available_date}</td>
  </tr>
</table>
<br />
<table class="forum" cellpadding="0" cellspacing="{page:cellspacing}" style="width:{page:width}">
  <tr>
    <td class="centerb">{version:msg}</td>
  </tr>
</table>
